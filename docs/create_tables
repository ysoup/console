CREATE TABLE `he_xun_information` (
	`id` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
	`title` VARCHAR(100) NOT NULL DEFAULT '"' COMMENT '标题',
	`source_link` VARCHAR(200) NOT NULL DEFAULT '"' COMMENT '资源链接',
	`content_id` BIGINT(20) NOT NULL COMMENT '内容id',
	`content` TEXT NOT NULL COMMENT '内容',
	`source_name` VARCHAR(10) NOT NULL DEFAULT '"' COMMENT '内容来源名称',
	`match_img` VARCHAR(1000) NOT NULL DEFAULT '"' COMMENT '图片链接',
	`author` VARCHAR(10) NOT NULL DEFAULT '"' COMMENT '作者',
	`crawler_date` VARCHAR(20) NOT NULL DEFAULT '"' COMMENT '网页上的时间',
	`create_time` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
	`update_time` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '修改时间',
	PRIMARY KEY (`id`)
)
COMMENT='和讯网数据表'
COLLATE='utf8_general_ci'
ENGINE=InnoDB;


CREATE TABLE `people_cn_information` (
	`id` INT(10) UNSIGNED NOT NULL AUTO_INCREMENT,
	`content_id` VARCHAR(50) NOT NULL DEFAULT '\'\'' COMMENT '内容id',
	`source_name` VARCHAR(50) NOT NULL DEFAULT '\'\'' COMMENT '资源名称',
	`author` VARCHAR(50) NOT NULL DEFAULT '\'\'' COMMENT '作者',
	`title` VARCHAR(100) NOT NULL DEFAULT '\'\'' COMMENT '标题',
	`content` TEXT NOT NULL COMMENT '内容',
	`crawler_url` VARCHAR(200) NOT NULL DEFAULT '\'\'' COMMENT '资讯链接',
	`img` VARCHAR(1000) NOT NULL DEFAULT '\'\'' COMMENT '图片url',
	`update_time` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
	`create_time` DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '创建时间',
	PRIMARY KEY (`id`)
)
COMMENT='人民网资讯数据表'
COLLATE='utf8_general_ci'
ENGINE=InnoDB;



===========================new_flash====================================
ALTER TABLE `new_flash`.`new_flash_information`
CHANGE COLUMN `content_id` `content_id` VARCHAR(128) NOT NULL DEFAULT ' ' COMMENT '内容编号' ;

ALTER TABLE `new_flash`.`new_flash_exclusive_information`
CHANGE COLUMN `content_id` `content_id` VARCHAR(128) NOT NULL DEFAULT ' ' COMMENT '内容编号' ;

ALTER TABLE `new_flash`.`new_flash_exclusive_information`
CHANGE COLUMN `tag` `tag` VARCHAR(128) NULL DEFAULT ' ' COMMENT '标签' ;
ALTER TABLE `new_flash`.`new_flash_information`
CHANGE COLUMN `news_url` `news_url` VARCHAR(500) NULL DEFAULT ' ' COMMENT '资讯生成快讯链接' ;


ALTER TABLE `new_flash`.`new_flash_information`
ADD COLUMN `tag` VARCHAR(128) NULL DEFAULT ' ' COMMENT '标签' AFTER `highlight_color`;























